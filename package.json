{
  "name": "flappy-bird",
  "version": "1.0.0",
  "description": "2P flappy bird in JS",
  "main": "dist/index.html",
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.4.0"
  },
  "devDependencies": {
    "@parcel/optimizer-cssnano": "^2.0.0-nightly.605",
    "@parcel/optimizer-htmlnano": "^2.0.0-nightly.605",
    "@parcel/packager-css": "^2.0.0-nightly.605",
    "@parcel/packager-html": "^2.0.0-nightly.605",
    "@parcel/transformer-css": "^2.0.0-nightly.605",
    "@parcel/transformer-html": "^2.0.0-nightly.605",
    "@parcel/transformer-postcss": "^2.0.0-nightly.605",
    "@parcel/transformer-posthtml": "^2.0.0-nightly.605",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.35",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "parcel": "^2.0.0-nightly.603",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:prettier": "prettier --check 'client/**/*.ts' 'server/**/*.ts'",
    "lint": "eslint . --ext .ts",
    "dev": "parcel public/index.html",
    "build": "parcel build public/index.html",
    "server": "parcel build public/index.html && NODE_ENV=production ts-node server/index.ts",
    "server:dev": "nodemon --watch 'server/**' --ext 'ts,json' --ignore 'server/**/*.spec.ts' --exec 'ts-node server/index.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avemike/flappy-bird.js.git"
  },
