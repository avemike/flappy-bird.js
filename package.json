{
  "name": "flappy-bird",
  "version": "1.0.0",
  "description": "2P flappy bird in JS",
  "dependencies": {
    "@parcel/transformer-svg-react": "^2.2.1",
    "express": "^4.17.1",
    "pino": "^6.11.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.4.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.8",
    "@parcel/config-default": "^2.0.0-nightly.748",
    "@parcel/resolver-glob": "^2.0.0-nightly.2338",
    "@parcel/runtime-js": "^2.0.0-nightly.748",
    "@parcel/runtime-react-refresh": "*",
    "@parcel/transformer-image": "^2.0.0-nightly.2370",
    "@parcel/transformer-react-refresh-babel": "*",
    "@parcel/transformer-react-refresh-wrap": "*",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "@types/express": "^4.17.13",
    "@types/node": "^14.14.41",
    "@types/pino": "^6.3.8",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-transition-group": "^4.4.2",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.36",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-plugin-styled-components": "^1.13.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "import-sort-style-module": "^6.0.0",
    "parcel": "^2.0.0-beta.3.1",
    "pino-pretty": "^5.1.0",
    "prettier": "^2.2.1",
    "react-icons": "^4.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.2.4",
    "uuid": "^3.4.0",
    "watch": "^1.0.2"
  },
  "scripts": {
    "eslint": "eslint client/ server/",
    "build": "parcel build public/index.html",
    "del-cache": "rm -drf .parcel-cache",
    "dev": "npm run del-cache && parcel watch public/index.html",
    "server": "nodemon --watch './**/*.ts' --exec 'ts-node -r tsconfig-paths/register' server/index.ts | pino-pretty",
    "prettier:fix": "prettier --write server/**/*.ts client/**/*.{ts,tsx}",
    "prettier:check": "prettier --check server/**/*.ts client/**/*.{ts,tsx}",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "tsc:check": "tsc --project ./tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avemike/flappy-bird.js.git"
  },
  "prettier": {
    "singleQuote": false,
    "semi": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 2,
    "importOrder": [
      "^~client/((\\w*)(?!/))$",
      "^~server/((\\w*)(?!/))$",
      "^~configs/(.*)$",
      "^~client/components/(.*)$",
      "^~client/controllers/(.*)$",
      "^~client/hooks/(.*)$",
      "^~client/utils/(.*)$",
      "^~client/view/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avemike/flappy-bird.js/issues"
  },
  "homepage": "https://github.com/avemike/flappy-bird.js#readme"
}
